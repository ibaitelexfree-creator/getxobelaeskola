{
    "mcpServers": {
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp",
                "--api-key",
                "YOUR_API_KEY"
            ]
        },
        "shadcn": {
            "command": "npx",
            "args": [
                "shadcn@latest",
                "mcp"
            ]
        },
        "docker": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-docker"
            ]
        },
        "qdrant": {
            "command": "npx",
            "args": [
                "-y",
                "@mhalder/qdrant-mcp-server"
            ],
            "env": {
                "QDRANT_URL": "http://localhost:6333",
                "QDRANT_API_KEY": "maestro_seguro_qdrant_2026"
            }
        }
        //other mcp servers
    }
}
// setup mcp server in  ~/.gemini\antigravity\mcp_config.json